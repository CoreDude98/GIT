<UserControl x:Class="anotherMultipleViewTest.Screens.MainMenu"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:anotherMultipleViewTest.Screens"
             mc:Ignorable="d" 
             d:DesignHeight="300" d:DesignWidth="600">
    <Grid Background="White">
        <Label x:Name="label" Content="Label" HorizontalAlignment="Left" Margin="70,65,0,0" VerticalAlignment="Top"/>
        <ComboBox x:Name="menuBox" HorizontalAlignment="Left" Margin="44,201,0,0" VerticalAlignment="Top" Width="120" Loaded="comboBoxLoaded" SelectionChanged="comboBox_SelectionChanged" />
        <Button x:Name="button" Content="Button" HorizontalAlignment="Left" Margin="156,91,0,0" VerticalAlignment="Top" Width="75" Click="button1Click"/>

    </Grid>
</UserControl>
